@model UCosmic.Www.Mvc.Areas.Identity.Models.SignInForm
@using (Html.BeginForm(
    MVC.Identity.SignIn.ActionNames.Post,
    MVC.Identity.SignIn.Name,
    new
    {
        area = MVC.Identity.Name,
        httpMethod = "POST",
    }
))
{
    @Html.AntiForgeryToken()
    <fieldset>
        <legend>Enter your Password</legend>
        <br />
        @Html.EditorForModel()
    </fieldset>
}
