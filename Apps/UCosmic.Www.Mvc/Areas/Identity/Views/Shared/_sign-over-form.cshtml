@model UCosmic.Www.Mvc.Areas.Identity.Models.SignOverForm
@using (Html.BeginForm(
    MVC.Identity.SignOver.ActionNames.Post,
    MVC.Identity.SignOver.Name,
    new
    {
        area = MVC.Identity.Name,
        httpMethod = "POST",
    }
))
{
    @Html.AntiForgeryToken()
    <fieldset>
        <legend>Sign On</legend>
        <br />
        @Html.EditorForModel()
    </fieldset>
}
